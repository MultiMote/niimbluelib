    "reverse-engineering",
    "thermal-printer",
    "label-printer",
    "niimbot",
    "niimbot-d110",
    "niimbot-b1",
    "bluetooth",
    "serial"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
